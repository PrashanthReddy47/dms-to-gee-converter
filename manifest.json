{
  "manifest_version": 3,
  "name": "DMS to GEE Converter",
  "description": "Convert DMS coordinates to Google Earth Engine ee.Geometry.Point format",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "DMS to GEE Converter"
  },
  "permissions": [
    "clipboardWrite"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
